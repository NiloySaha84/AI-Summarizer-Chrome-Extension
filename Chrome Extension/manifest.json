{
  "manifest_version": 3,
  "name": "AI Summarizer",
  "version": "1.0",
  "description": "Summarizes the article/Youtube video on the current page.",
  "permissions": ["activeTab", "scripting"],
  "host_permissions": ["<all_urls>"],
  "action": {
    "default_popup": "index.html",
    "default_icon": "chrome-ext.png"
  }
}